def rev_string():
    my_str = "Hello World"
    str_len = len(my_str)
    out_put = []
    for i in my_str:
        str_len = str_len - 1
        out_put.append(my_str[str_len])
    print(out_put)


def remove_duplicate(dup_array):
    """To find duplicate element in string"""
    #dup_array = [1, 2, 1, 2, 3, 4, 5, 3]
    dup_removed = []
    for num in dup_array:
        if num not in dup_removed:
            dup_removed.append(num)
    print(dup_removed)

def find_duplicate(dup_array):
    """ To find the duplicate element in string"""
    #dup_array = [1, 2, 1, 2, 3, 4, 5, 3]
    dup_removed = []
    dup_elem = []
    for num in dup_array:
        if num == num in dup_removed:
            dup_elem.append(num)
        else:
            dup_removed.append(num)
    print(dup_elem)
