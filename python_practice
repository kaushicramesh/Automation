def fibonacci():
    """ Finding Fibonacci series for first 10 number"""
    a, b = 1, 1
    j = 1
    for i in range(0, 11):
        print ("The fib of {} is {}".format(j, a))
        j += 1
        a, b = b, a + b

def powered():
    """ Find multiplying of 100 for first 10 even number"""
    #list = [1, 3, 4, 5, 2, 9, 7]
    powered = [num*100 for num in range(2, 21, 2)]
    print(powered)

